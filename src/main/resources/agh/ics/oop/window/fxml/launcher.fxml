<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.GridPane?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="agh.ics.oop.window.controller.Launcher"
            minWidth="250.0" minHeight="100.0">
    <center>
        <GridPane hgap="10.0" vgap="10.0">
            <Button GridPane.columnIndex="0" onAction="#launchConfigurator">edit configuration</Button>
            <Button GridPane.columnIndex="1" onAction="#launchViewer">launch simulation</Button>
        </GridPane>
    </center>
</BorderPane>
